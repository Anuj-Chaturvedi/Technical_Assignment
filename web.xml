<?xml version="1.0" encoding="utf-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"

         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"

         version="3.1">

       
  <servlet>

    <servlet-name>UploadHandlerServlet</servlet-name>

    <servlet-class>Servlets.UploadHandlerServlet</servlet-class>

  </servlet>

  <servlet>

    <servlet-name>LoginServlet</servlet-name>

    <servlet-class>Servlets.LoginServlet</servlet-class>

  </servlet>
  
  <servlet>

    <servlet-name>MigrationServlet</servlet-name>

    <servlet-class>Servlets.MigrationServlet</servlet-class>

  </servlet>

  
  <servlet-mapping>

    <servlet-name>UploadHandlerServlet</servlet-name>

    <url-pattern>/UploadHandlerServlet</url-pattern>

  </servlet-mapping>

  <servlet-mapping>

    <servlet-name>LoginServlet</servlet-name>

    <url-pattern>/LoginServlet</url-pattern>

  </servlet-mapping>
  
  <servlet-mapping>

    <servlet-name>MigrationServlet</servlet-name>

    <url-pattern>/MigrationServlet</url-pattern>

  </servlet-mapping>


  <welcome-file-list>

    <welcome-file>index.html</welcome-file>

    <welcome-file>index.jsp</welcome-file>

  </welcome-file-list>

</web-app>
